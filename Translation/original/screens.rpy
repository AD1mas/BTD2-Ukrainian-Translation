screen say(who, what, side_image=None, two_window=False):


    if not two_window:


        window:
            id "window"

            has vbox
            style "say_vbox"

            if who:
                text who id "who"

            text what id "what"

    else:


        vbox:
            style "say_two_window_vbox"

            if who:
                window:
                    style "say_who_window"

                    text who:
                        id "who"

            window:
                id "window"

                has vbox
                style "say_vbox"

                text what id "what"


    if side_image:
        add side_image xalign 0.21 yalign 0.842
    else:
        add SideImage() xalign 0.0 yalign 1.0


    use quick_menu








screen choice(items):

    window:
        style "menu_window"
        xalign 0.5
        yalign 0.5

        has vbox
        style "menu"
        spacing 2

        for caption, action, chosen in items:

            if action:

                button:
                    action action
                    style "menu_choice_button"

                    text caption style "menu_choice"

            else:
                text caption style "menu_caption"

